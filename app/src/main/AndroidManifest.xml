<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.livejournal.karino2.tobinq.app" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.livejournal.karino2.tobinq.app.ScriptListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.livejournal.karino2.tobinq.app.ScratchActivity"
            android:label="@string/title_activity_scratch" >
        </activity>
        <activity
            android:name="com.livejournal.karino2.tobinq.app.SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name="com.livejournal.karino2.tobinq.app.EvalActivity"
            android:label="@string/title_activity_eval"
            android:parentActivityName="com.livejournal.karino2.tobinq.app.ScriptListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ScriptListActivity" />
        </activity>
    </application>

</manifest>
